{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Does a higher GDP mean a higher vaccinated population?",
  "width": 500,
  "height": 300,
  "data": {"url": "cleaned_data_2020_10_1.csv"},
  "mark": "circle",
  "encoding": {
    "x": {"field": "Logged GDP per capita", 
    "type": "quantitative",
    "axis": {"tickCount": 6},
    "scale": { "domain": [6, 12]}
  },
    "y": {"field": "% of population vaccinated", 
    "type": "quantitative",
    "axis": {"tickCount": 10},
      "scale": {"domain": [1, 100]}
  },
    "color": {"field": "Regional indicator", "type": "nominal"},
    "tooltip": [
      {"field": "Country", "type": "nominal", "title": "Country"},
      {"field": "Logged GDP per capita", "type": "quantitative"},
      {"field": "% of population vaccinated", "type": "quantitative"}
    ]
  }

}
